<%= semantic_form_for @event do |f| %>

    <%= f.input :title %>
    <%= f.input :description, :as => :ckeditor %>
    <%= f.input :venue %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :start, as: :datetime_select %>
    <%= f.input :url %>
    <%= f.input :tag_list %>
    <%= f.button :submit %>

<% end %>
